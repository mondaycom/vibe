{
  "name": "@vibe/core",
  "version": "3.85.0",
  "description": "Official monday.com UI resources for application development in React.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mondaycom/vibe.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/mondaycom/vibe/issues"
  },
  "homepage": "https://github.com/mondaycom/vibe#readme",
  "author": "monday.com",
  "license": "MIT",
  "type": "module",
  "main": "dist/src/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./meta": {
      "import": "./dist/metadata.json",
      "default": "./dist/metadata.json"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/src/index.js",
      "default": "./dist/src/index.js"
    },
    "./interactionsTests": {
      "types": "./dist/tests/interactions-utils.d.ts",
      "import": "./dist/src/tests/interactionsTests.js",
      "default": "./dist/src/tests/interactionsTests.js"
    },
    "./testIds": {
      "types": "./dist/tests/test-ids-utils.d.ts",
      "import": "./dist/src/tests/testIds.js",
      "default": "./dist/src/tests/testIds.js"
    },
    "./next": {
      "types": "./dist/components/next.d.ts",
      "import": "./dist/src/components/next.js",
      "default": "./dist/src/components/next.js"
    },
    "./tokens": {
      "types": "./dist/tokens/tokens.d.ts",
      "import": "./dist/tokens/tokens.css",
      "default": "./dist/tokens/tokens.css"
    },
    "./mockedClassNames": {
      "import": "./dist/mocked_classnames/src/index.js",
      "default": "./dist/mocked_classnames/src/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "interactionsTests": [
        "./dist/tests/interactions-utils.d.ts"
      ],
      "testIds": [
        "./dist/tests/test-ids-utils.d.ts"
      ],
      "next": [
        "./dist/components/next.d.ts"
      ],
      "tokens": [
        "./dist/tokens/tokens.d.ts"
      ]
    }
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:update": "vitest run -u",
    "test:coverage": "vitest run --coverage",
    "start": "yarn build:esm -- --watch",
    "build": "yarn build:esm && yarn build:mocked-classnames && yarn build:metadata",
    "build:esm": "rollup -c",
    "build:mocked-classnames": "[ \"$SKIP_RELEASE_ARTIFACTS\" = \"true\" ] || mock_classnames=on rollup -c",
    "build:metadata": "[ \"$SKIP_RELEASE_ARTIFACTS\" = \"true\" ] || tsx src/scripts/generate-metadata.ts",
    "link-local": "npm link && npm start",
    "plop": "plop",
    "lint": "eslint \"./src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "yarn lint -- --fix",
    "stylelint": "stylelint \"**/*.scss\"",
    "stylelint:fix": "yarn stylelint -- --fix"
  },
  "dependencies": {
