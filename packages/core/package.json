{
  "name": "monday-ui-react-core",
  "version": "2.131.1",
  "description": "Official monday.com UI resources for application development in React.js",
  "main": "./dist/main.js",
  "types": "./dist/types.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/esm/src/index.js",
      "default": "./dist/main.js"
    },
    "./dist/": "./dist/",
    "./icons": {
      "types": "./dist/esm/components/Icon/Icons/index.d.ts",
      "import": "./dist/esm/src/components/Icon/Icons/icons.js",
      "default": "./dist/allIcons.js"
    },
    "./interactionsTests": {
      "types": "./dist/esm/tests/interactions-utils.d.ts",
      "import": "./dist/esm/src/tests/interactionsTests.js",
      "default": "./dist/esm/src/tests/interactionsTests.js"
    },
    "./testIds": {
      "types": "./dist/esm/tests/test-ids-utils.d.ts",
      "import": "./dist/esm/src/tests/testIds.js",
      "default": "./dist/esm/src/tests/testIds.js"
    },
    "./storybookComponents": "./dist/storybook/index.js",
    "./next": {
      "types": "./dist/esm/next/next.d.ts",
      "import": "./dist/esm/src/next/next.js",
      "default": "./dist/esm/src/next/next.js"
    },
    "./tokens": "./dist/tokens/tokens.css",
    "./mockedClassNames": {
      "import": "./dist/mocked_classnames_esm/src/index.js"
    },
    "./types": "./dist/esm/types/index.d.ts"
  },
  "typesVersions": {
    "*": {
      "icons": [
        "./dist/esm/components/Icon/Icons/index.d.ts"
      ],
      "interactionsTests": [
        "./dist/esm/tests/interactions-utils.d.ts"
      ],
      "testIds": [
        "./dist/esm/tests/test-ids-utils.d.ts"
      ],
      "next": [
        "./dist/esm/next/next.d.ts"
      ],
      "types": [
        "./dist/esm/types/index.d.ts"
      ]
    }
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:update": "yarn test -u",
    "test:coverage": "yarn test -- --coverage",
    "start": "yarn build:react-icons && yarn build:commonjs -- --watch",
    "build": "yarn build:react-icons && yarn build:storybook-exports && yarn build:commonjs && yarn build:esm && yarn build:esm:mock-classnames && yarn build:types",
    "build:exports": "./scripts/build-exports.js",
    "build:types": "./scripts/build-types.js",
    "build:storybook-exports": "./scripts/build-storybook-exports.js",
    "build:commonjs": "webpack --mode production --config webpack/default.config.js",
    "build:esm": "rollup -c",
    "build:esm:mock-classnames": "mock_classnames=on rollup -c",
    "build:react-icons": "svg2react-icon --typescript --keep-colors ../../node_modules/monday-ui-style/src/Icons src/components/Icon/Icons",
    "storybook": "storybook dev -p 7008",
    "build-storybook": "yarn build:storybook-exports && storybook build -o static_storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "chromatic:local": "chromatic -t $CHROMATIC_PROJECT_TOKEN",
    "link-local": "npm link && npm start",
    "plop": "plop",
    "lint": "eslint \"./src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "yarn lint -- --fix",
    "stylelint": "stylelint \"**/*.scss\"",
    "stylelint:fix": "yarn stylelint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mondaycom/vibe.git"
  },
  "author": "",
  "license": "MIT",
